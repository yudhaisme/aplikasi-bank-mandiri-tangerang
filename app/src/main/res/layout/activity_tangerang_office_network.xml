<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.prayudha.asdi.mandiritangerangservice.TangerangOfficeNetwork"
    android:background="@drawable/background4">

    <ImageView
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:layout_marginTop="15dp"
        android:id="@+id/judul1"
        android:src="@drawable/logoatas"
        android:layout_marginBottom="30dp"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/judul"
        android:textSize="15dp"
        android:textColor="#fce400"
        android:textStyle="bold"
        android:layout_marginBottom="30dp"
        android:layout_below="@+id/judul1"
        android:text="KANTOR CABANG BANK MANDIRI"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:layout_marginBottom="15dp"
        android:id="@+id/kabupaten_tangerang"
        android:src="@drawable/kabupaten_tangerang2"
        android:layout_below="@+id/judul"
        android:layout_centerHorizontal="true"
        android:clickable="true"
        />

    <ImageView
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:layout_marginBottom="15dp"
        android:id="@+id/kota_tangerang"
        android:src="@drawable/kota_tangerang2"
        android:layout_below="@+id/kabupaten_tangerang"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:layout_marginBottom="15dp"
        android:id="@+id/tangerang_selatan"
        android:src="@drawable/tangerang_selatan2"
        android:layout_below="@+id/kota_tangerang"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
